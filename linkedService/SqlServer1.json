{
	"name": "SqlServer1",
	"type": "Microsoft.DataFactory/factories/linkedservices",
	"properties": {
		"parameters": {
			"DBServerName": {
				"type": "string"
			},
			"DBName": {
				"type": "string"
			},
			"DBUserName": {
				"type": "string"
			},
			"DBPassword": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "SqlServer",
		"typeProperties": {
			"connectionString": "Integrated Security=False;Data Source=@{linkedService().DBServerName};Initial Catalog=@{linkedService().DBName};User ID=@{linkedService().DBUserName};Password=@{linkedService().DBPassword}"
		}
	}
}